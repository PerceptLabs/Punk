{
  "name": "@punk/extended",
  "version": "1.0.0",
  "description": "Extended component rig for Punk Framework - curated collection of component wrappers",
  "keywords": [
    "punk",
    "framework",
    "components",
    "rig",
    "ui",
    "react"
  ],
  "author": "PerceptLabs",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/presets.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/presets.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@punk/core": "workspace:*",
    "@punk/component-chart": "workspace:*",
    "@punk/component-table": "workspace:*",
    "@punk/component-mermaid": "workspace:*",
    "@punk/component-richtext": "workspace:*",
    "@punk/component-code": "workspace:*",
    "@punk/component-filedrop": "workspace:*",
    "@punk/component-date": "workspace:*",
    "@punk/component-command": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
